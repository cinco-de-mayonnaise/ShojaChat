<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<HBox alignment="CENTER_LEFT" prefWidth="680.0" spacing="15.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="Label_TimeStamp" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="9:15 AM" HBox.hgrow="NEVER">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <TextFlow prefHeight="17.0" prefWidth="357.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="Text_MessageAuthor" strokeType="OUTSIDE" strokeWidth="0.0" text="Message_Author: ">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
            <Text layoutX="10.0" layoutY="10.0" strokeType="OUTSIDE" strokeWidth="0.0" text=" " />
            <Text fx:id="Text_MessageContents" layoutX="10.0" layoutY="10.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hi! I'm typing some pretty rad mesaages here lmao xd\\n\\ntasdfasdfasdfasdfasdfasdf asdfasdof lasdlfkj lkasjdf jklasdfkljasdf es">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
